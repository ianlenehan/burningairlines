<div class="container">
  <h1>home page</h1>
  <div id="main"></div>

<h1>Home Page</h1>

  <script id="searchFlightTemplate" type="text/template">

    <div id="from"></div>
    <div id="to"></div>
    <button id="cancel"></button>
    <button id="save"></button>

    <button id="searchFlights">Search</button>
  </script>

  <script id="flightViewTemplate" type="text/template">

    <button></button>


    <dl>
      <dt>origin:</dt>
      <dd><%= origin %></dd>

      <dt>destination:</dt>
      <dd><%= destination %></dd>

      <dt>date:</dt>
      <dd><%= date %></dd>

      <dt>plane:</dt>
      <dd id="planeDescription"></dd>
    </dl>

    <div id="flightSeating"></div>
  </script>

  <script id="planeDescriptionTemplate" type="text/template">
    <%= name %> which has <%= rows %> rows and <%= columns %> columns of seats.
  </script>

</div>
